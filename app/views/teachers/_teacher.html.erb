<div id="<%= dom_id teacher %>">
  <tr>
    <td> <%= teacher.name %> </td>
    <td> <%= teacher.date_of_birth %> </td>    
    <td> <%= teacher.address %> </td>
    <td> <%= teacher.phone%> </td>
    <td> <%= teacher.email%> </td>
    <td> <%= teacher.rg %> </td>
    <td> <%= teacher.cpf %> </td>
  
    <td> 
      <%= link_to teacher_path(teacher) do %>
        <i class="fas fa-eye"></i>
      <%end%>
    </td>

    <td> 
      <%= link_to edit_teacher_path(teacher) do %>
        <i class="fas fa-edit"></i>
      <%end%>
    </td>
  </tr>
</div>