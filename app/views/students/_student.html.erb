<div id="<%= dom_id student %>">
  <tr>
    <td> <%= student.name %> </td>
    <td> <%= student.cpf %> </td>
    <td> <%= student.date_of_birth %> </td>
    <td> <%= student.address %> </td>
    <td> <%= student.phone%> </td>
    <td> <%= student.email%> </td>
  
    <td> 
      <%= link_to student_path(student) do %>
        <i class="fas fa-eye"></i>
      <%end%>
    </td>

    <td> 
      <%= link_to edit_student_path(student) do %>
        <i class="fas fa-edit"></i>
      <%end%>
    </td>
  </tr>
</div>
