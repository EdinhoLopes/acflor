<div id="<%= dom_id course %>">
  <tr>
    <td> <%= course.name %> </td>

    <td> 
      <%= link_to course_path(course) do %>
        <i class="fas fa-eye"></i>
      <%end%>
    </td>

    <td> 
    <%= link_to edit_course_path(course) do %>
      <i class="fas fa-edit"></i>
    <%end%>
    </td>
  </tr>
</div>
